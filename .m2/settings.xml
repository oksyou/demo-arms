    <servers>
        <server>
            <username>${env.MAVEN_REPO_USER}</username>
            <password>${env.MAVEN_REPO_PASS}</password>
            <id>repo</id>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <mirrorOf>*</mirrorOf>
            <name>repo</name>
            <url>https://repo.croc.ru/artifactory/ctp.jxfw</url>
            <id>repo</id>
        </mirror>
    </mirrors>
    <profiles>
     <profile>
      <repositories>
        <repository>
          <id>repo</id>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
	  <url>https://repo.croc.ru/artifactory/ctp.jxfw</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>repo</id>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>            
	  <url>https://repo.croc.ru/artifactory/ctp.jxfw</url>
        </pluginRepository>
      </pluginRepositories>
      <id>default</id>
     </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>default</activeProfile>
    </activeProfiles>
</settings>
